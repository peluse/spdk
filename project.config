[project]
	description = Storage Performance Development Kit - http://spdk.io
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Paul Luse (peluse)
[access "refs/for/refs/*"]
	read = group user/Paul Luse (peluse)
	push = group user/Paul Luse (peluse)
	label-Code-Review = -2..+2 group user/Paul Luse (peluse)
	label-Verified = -1..+1 group user/Paul Luse (peluse)
	rebase = group user/Paul Luse (peluse)
	removeReviewer = group user/Paul Luse (peluse)
	submit = group user/Paul Luse (peluse)
[access "refs/heads/*"]
	read = group user/Paul Luse (peluse)
	create = group user/Paul Luse (peluse)
	push = +force group user/Paul Luse (peluse)
	pushMerge = group user/Paul Luse (peluse)
[access "refs/tags/*"]
	push = group user/Paul Luse (peluse)
	push = +force group user/Paul Luse (peluse)
